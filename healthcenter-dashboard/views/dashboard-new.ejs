                                        <p class="loading-text">Loading all appointments...</p>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <!-- Messages Content -->
            <div id="messages-content" class="dashboard-content" style="display: none;">
                <!-- Messages Header -->
                <div class="messages-header mb-4">
                    <div class="row align-items-center">
                        <div class="col-md-8">
                            <div class="header-content">
                                <h2 class="mb-2">
                                    <i class="fas fa-comments text-primary me-3"></i>
                                    Messages Center
                                </h2>
                                <p class="text-muted mb-0">Communicate with patients and manage clinic communications</p>
                            </div>
                        </div>
                        <div class="col-md-4 text-end">
                            <div class="header-actions">
                                <button class="btn btn-primary" onclick="messagesPage.composeNewMessage()">
                                    <i class="fas fa-plus me-2"></i>New Message
                                </button>
                            </div>
                        </div>
                    </div>
                </div>

                <!-- Messages Statistics -->
                <div class="row mb-4">
                    <div class="col-xl-3 col-md-6 mb-3">
                        <div class="stat-card-messages">
                            <div class="stat-icon bg-primary">
                                <i class="fas fa-inbox"></i>
                            </div>
                            <div class="stat-content">
                                <h3 id="totalMessages">0</h3>
                                <p>Total Messages</p>
                            </div>
                        </div>
                    </div>
                    <div class="col-xl-3 col-md-6 mb-3">
                        <div class="stat-card-messages">
                            <div class="stat-icon bg-warning">
                                <i class="fas fa-envelope"></i>
                            </div>
                            <div class="stat-content">
                                <h3 id="unreadMessages">0</h3>
                                <p>Unread</p>
                            </div>
                        </div>
                    </div>
                    <div class="col-xl-3 col-md-6 mb-3">
                        <div class="stat-card-messages">
                            <div class="stat-icon bg-success">
                                <i class="fas fa-reply"></i>
                            </div>
                            <div class="stat-content">
                                <h3 id="sentMessages">0</h3>
                                <p>Sent</p>
                            </div>
                        </div>
                    </div>
                    <div class="col-xl-3 col-md-6 mb-3">
                        <div class="stat-card-messages">
                            <div class="stat-icon bg-info">
                                <i class="fas fa-clock"></i>
                            </div>
                            <div class="stat-content">
                                <h3 id="pendingReplies">0</h3>
                                <p>Pending Replies</p>
                            </div>
                        </div>
                    </div>
                </div>

                <!-- Messages Management -->
                <div class="row">
                    <div class="col-12">
                        <div class="content-card">
                            <div class="card-header d-flex justify-content-between align-items-center">
                                <div class="header-content">
                                    <h5 class="mb-0">
                                        <i class="fas fa-envelope text-primary me-2"></i>
                                        Messages
                                    </h5>
                                    <p class="text-muted mb-0 mt-1">Manage your clinic communications</p>
                                </div>
                                <div class="header-actions d-flex gap-2">
                                    <div class="btn-group" role="group">
                                        <button type="button" class="btn btn-outline-primary btn-sm active" data-filter="all">
                                            <i class="fas fa-list me-1"></i>All
                                        </button>
                                        <button type="button" class="btn btn-outline-warning btn-sm" data-filter="unread">
                                            <i class="fas fa-envelope me-1"></i>Unread
                                        </button>
                                        <button type="button" class="btn btn-outline-success btn-sm" data-filter="sent">
                                            <i class="fas fa-reply me-1"></i>Sent
                                        </button>
                                        <button type="button" class="btn btn-outline-info btn-sm" data-filter="urgent">
                                            <i class="fas fa-exclamation me-1"></i>Urgent
                                        </button>
                                    </div>
                                </div>
                            </div>
                            <div class="card-body p-0">
                                <div class="messages-container" id="messagesList">
                                    <!-- Messages will be loaded dynamically -->
                                    <div class="loading-state">
                                        <div class="spinner-container">
                                            <div class="spinner-border text-primary" role="status">
                                                <span class="visually-hidden">Loading...</span>
                                            </div>
                                        </div>
                                        <p class="loading-text">Loading messages...</p>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </main>
    </div>
    
