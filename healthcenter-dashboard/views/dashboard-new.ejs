<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Dashboard - Health Center</title>
    
    <!-- Bootstrap CSS -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.min.css" rel="stylesheet">
    
    <!-- Font Awesome -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css">
    
    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/css2?family=Ubuntu:wght@300;400;500;600;700;800&family=Inter:wght@300;400;500;600;700;800&display=swap" rel="stylesheet">
    
    <!-- Custom CSS -->
    <link rel="stylesheet" href="/css/dashboard.css">
    <link rel="stylesheet" href="/css/dashboard-styles.css">
    <link rel="stylesheet" href="/css/chat-professional.css">
    
    <!-- Chart.js -->
    <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
    
    <!-- Audio element for notifications -->
    <audio id="notificationSound" preload="auto">
        <source src="/assets/notification.mp3" type="audio/mpeg">
        <source src="/assets/notification.wav" type="audio/wav">
    </audio>
    
    <!-- Audio element for appointment sound -->
    <audio id="appointmentSound" preload="auto">
        <source src="/assets/appointment.mp3" type="audio/mpeg">
        <source src="/assets/appointment.wav" type="audio/wav">
    </audio>
</head>
<body>
    <div class="dashboard-container">
        <!-- Sidebar -->
        <nav class="sidebar">
            <div class="sidebar-header">
                <div class="logo">
                    <i class="fas fa-hospital-alt"></i>
                    <span>HomeCare Plus</span>
                </div>
            </div>
            
            <div class="sidebar-user">
                <div class="user-avatar">
                    <i class="fas fa-user-md"></i>
                </div>
                <div class="user-info">
                    <h6><%= user.clinicName || 'Admin' %></h6>
                    <span>Administrator</span>
                </div>
            </div>
            
            <ul class="sidebar-nav">
                <li class="nav-item active">
                    <a href="#dashboard" class="nav-link">
                        <i class="fas fa-tachometer-alt"></i>
                        <span>Dashboard</span>
                    </a>
                </li>
                <li class="nav-item">
                    <a href="#appointments" class="nav-link">
                        <i class="fas fa-calendar-check"></i>
                        <span>Appointments</span>
                    </a>
                </li>
                <li class="nav-item">
                    <a href="#messages" class="nav-link" id="messages-link">
                        <i class="fas fa-comments" id="chatIcon"></i>
                        <span>Messages</span>
                    </a>
                </li>
            </ul>
            
            <div class="sidebar-footer">
                <a href="/logout" class="nav-link">
                    <i class="fas fa-sign-out-alt"></i>
                    <span>Logout</span>
                </a>
            </div>
        </nav>
        
        <!-- Main Content -->
        <main class="main-content">
            <!-- Top Header -->
            <header class="top-header">
                <div class="header-left">
                    <button class="sidebar-toggle">
                        <i class="fas fa-bars"></i>
                    </button>
                    <div class="header-title">
                        <h1 class="mb-1"><i class="fas fa-tachometer-alt me-2"></i>Dashboard</h1>
                        <p class="text-muted mb-0" style="font-size:1.1rem;font-weight:500;">Welcome back, <span style="color:var(--primary-color);font-weight:700"><%= user.clinicName || 'Admin' %></span>! Here is your professional overview for today.</p>
                    </div>
                </div>
                <div class="header-right d-flex align-items-center gap-3">
                    <div class="d-flex flex-column align-items-end me-3">
                        <span class="fw-bold" style="color:var(--primary-color)">Today: <%= new Date().toLocaleDateString('en-US', {weekday:'long', year:'numeric', month:'short', day:'numeric'}) %></span>
                        <span class="text-secondary" style="font-size:0.95rem;">Stay productive!</span>
                    </div>
                    <div class="header-actions d-flex align-items-center gap-2">
                        <div class="user-menu d-flex align-items-center gap-2">
                            <img src="<%= user.profileImageUrl || '/assets/hospital.PNG' %>" alt="User" class="user-avatar-small" id="headerUserAvatar">
                            <span class="fw-bold"><%= user.clinicName || 'Admin' %></span>
                        </div>
                    </div>
                </div>
            </header>
            
            <!-- Notification Toast Container -->
            <div id="notificationContainer" class="notification-toast-container"></div>
            
            <!-- Dashboard Content -->
            <div class="dashboard-content">
                <!-- Stats Cards -->
                <div class="row mb-4">
                    <div class="col-xl-6 col-md-6 mb-4">
                        <div class="stat-card">
                            <div class="stat-icon bg-primary">
                                <i class="fas fa-calendar-check"></i>
                            </div>
                            <div class="stat-content">
                                <h3>24</h3>
                                <p>Today's Appointments</p>
                                <span class="stat-change positive">
                                    <i class="fas fa-arrow-up"></i> 12%
                                </span>
                            </div>
                        </div>
                    </div>
                    <div class="col-xl-6 col-md-6 mb-4">
                        <div class="stat-card">
                            <div class="stat-icon bg-success">
                                <i class="fas fa-user-check"></i>
                            </div>
                            <div class="stat-content">
                                <h3>18</h3>
                                <p>Confirmed Appointments</p>
                                <span class="stat-change positive">
                                    <i class="fas fa-arrow-up"></i> 8%
                                </span>
                            </div>
                        </div>
                    </div>
                </div>
                
                <!-- Main Content Row -->
                <div class="row">
                    <!-- Appointments List -->
                    <div class="col-12">
                        <div class="content-card">
                            <div class="card-header d-flex justify-content-between align-items-center">
                                <div class="header-content">
                                    <h5 class="mb-0">
                                        <i class="fas fa-calendar-alt text-primary me-2"></i>
                                        Upcoming Appointments
                                    </h5>
                                    <p class="text-muted mb-0 mt-1">Manage and view your clinic appointments</p>
                                </div>
                                <div class="header-actions">
                                    <button class="btn btn-outline-primary btn-sm me-2" onclick="dashboardAppointments.refresh()">
                                        <i class="fas fa-refresh me-1"></i> Refresh
                                    </button>
                                    <button class="btn btn-primary btn-sm" onclick="navigateToAppointments()">
                                        <i class="fas fa-list me-1"></i> View All
                                    </button>
                            </div>
                                        </div>
                            <div class="card-body p-0">
                                <div class="appointments-container" id="appointmentsList">
                                    <!-- Appointments will be loaded dynamically -->
                                    <div class="loading-state">
                                        <div class="spinner-container">
                                            <div class="spinner-border text-primary" role="status">
                                                <span class="visually-hidden">Loading...</span>
                                        </div>
                                        </div>
                                        <p class="loading-text">Loading appointments...</p>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                                        </div>
                                    </div>
                                    
            <!-- Settings Content (Hidden by default) -->
            <div class="settings-content" id="settings-content" style="display: none;">
                <!-- Settings Header -->
                <div class="settings-header mb-4">
                    <div class="row align-items-center">
                        <div class="col-md-8">
                            <div class="header-content">
                                <h2 class="mb-2">
                                    <i class="fas fa-cog text-primary me-3"></i>
                                    Clinic Settings
                                </h2>
                                <p class="text-muted mb-0">Manage your clinic information, preferences, and account settings</p>
                                        </div>
                                        </div>
                        <div class="col-md-4 text-end">
                            <div class="header-actions">
                                <button class="btn btn-primary" onclick="settingsPage.saveAllSettings()">
                                    <i class="fas fa-save me-2"></i>Save All Changes
                                            </button>
                            </div>
                        </div>
                                        </div>
                                    </div>
                                    
                <!-- Settings Statistics -->
                <div class="row mb-4">
                    <div class="col-xl-3 col-md-6 mb-3">
                        <div class="stat-card-settings">
                            <div class="stat-icon bg-primary">
                                <i class="fas fa-user-check"></i>
                                        </div>
                            <div class="stat-content">
                                <h3 id="profileCompletion">85%</h3>
                                <p>Profile Complete</p>
                                        </div>
                                        </div>
                                    </div>
                    <div class="col-xl-3 col-md-6 mb-3">
                        <div class="stat-card-settings">
                            <div class="stat-icon bg-success">
                                <i class="fas fa-stethoscope"></i>
                                </div>
                            <div class="stat-content">
                                <h3 id="activeServices">5</h3>
                                <p>Active Services</p>
                            </div>
                        </div>
                    </div>
                    <div class="col-xl-3 col-md-6 mb-3">
                        <div class="stat-card-settings">
                            <div class="stat-icon bg-warning">
                                <i class="fas fa-clock"></i>
                            </div>
                            <div class="stat-content">
                                <h3 id="workingDays">6</h3>
                                <p>Working Days</p>
                                </div>
                            </div>
                        </div>
                    <div class="col-xl-3 col-md-6 mb-3">
                        <div class="stat-card-settings">
                            <div class="stat-icon bg-info">
                                <i class="fas fa-shield-alt"></i>
                    </div>
                            <div class="stat-content">
                                <h3 id="securityLevel">High</h3>
                                <p>Security Level</p>
                </div>
            </div>
                    </div>
                    </div>
                    
                <!-- Settings Management -->
                    <div class="row">
                        <div class="col-12">
                        <div class="content-card">
                            <div class="card-header d-flex justify-content-between align-items-center">
                                <div class="header-content">
                                    <h5 class="mb-0">
                                        <i class="fas fa-sliders-h text-primary me-2"></i>
                                        Settings Configuration
                                    </h5>
                                    <p class="text-muted mb-0 mt-1">Configure your clinic settings and preferences</p>
                                </div>
                                <div class="header-actions d-flex gap-2">
                                    <div class="btn-group" role="group">
                                        <button type="button" class="btn btn-outline-primary btn-sm active" data-settings-tab="profile">
                                            <i class="fas fa-user me-1"></i>Profile
                                            </button>
                                        <button type="button" class="btn btn-outline-success btn-sm" data-settings-tab="contact">
                                            <i class="fas fa-phone me-1"></i>Contact
                                            </button>
                                        <button type="button" class="btn btn-outline-warning btn-sm" data-settings-tab="services">
                                            <i class="fas fa-stethoscope me-1"></i>Services
                                            </button>
                                        <button type="button" class="btn btn-outline-info btn-sm" data-settings-tab="schedule">
                                            <i class="fas fa-calendar-alt me-1"></i>Schedule
                                            </button>
                                        <button type="button" class="btn btn-outline-secondary btn-sm" data-settings-tab="documents">
                                            <i class="fas fa-file-upload me-1"></i>Documents
                                            </button>
                                        <button type="button" class="btn btn-outline-danger btn-sm" data-settings-tab="security">
                                            <i class="fas fa-shield-alt me-1"></i>Security
                                            </button>
                                </div>
                                </div>
                            </div>
                            <div class="card-body p-0">
                                <div class="settings-container">
                                    <!-- Profile Settings Tab -->
                                    <div class="settings-tab active" id="profile-tab">
                                        <div class="settings-tab-content">
                                            <div class="row">
                                                <div class="col-md-4">
                                                    <div class="profile-image-section text-center">
                                                        <div class="profile-image-container">
                                                            <img id="profileImage" src="/assets/hospital.PNG" alt="Clinic Profile" class="profile-image">
                                                            <div class="profile-image-overlay">
                                                                <button class="btn btn-light btn-sm" onclick="settingsPage.changeProfileImage()">
                                                                    <i class="fas fa-camera"></i>
                                                                </button>
                                                            </div>
                                                        </div>
                                                        <input type="file" id="profileImageInput" accept="image/*" style="display: none;">
                                                        <p class="text-muted mt-2">Click to change profile image</p>
                                                        <div class="profile-status mt-3">
                                                            <span class="badge bg-success">Active</span>
                                                            <span class="badge bg-primary ms-2">Verified</span>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-md-8">
                                                    <form id="profileForm" class="profile-edit-form">
                                                        <div class="row">
                                                            <div class="col-md-6">
                                                                <div class="form-group">
                                                                    <label for="clinicName">
                                                                        <i class="fas fa-hospital text-primary me-2"></i>
                                                                        Clinic Name <span class="text-danger">*</span>
                                                                    </label>
                                                                    <input type="text" class="form-control" id="clinicName" name="clinicName" value="<%= user.clinicName || user.name || 'Clinic Name' %>" required>
                                                                </div>
                                                            </div>
                                                            <div class="col-md-6">
                                                                <div class="form-group">
                                                                    <label for="clinicEmail">
                                                                        <i class="fas fa-envelope text-primary me-2"></i>
                                                                        Email Address
                                                                    </label>
                                                                    <input type="email" class="form-control" id="clinicEmail" name="clinicEmail" value="<%= user.email %>" readonly>
                                                                    <small class="form-text text-muted">Email cannot be changed after registration</small>
                                                                </div>
                                                            </div>
                                                        </div>
                                                        <div class="form-group">
                                                            <label for="clinicAbout">
                                                                <i class="fas fa-info-circle text-primary me-2"></i>
                                                                About Clinic <span class="text-danger">*</span>
                                                            </label>
                                                            <textarea class="form-control" id="clinicAbout" name="clinicAbout" rows="4" placeholder="Describe your clinic's mission and services..." required><%= user.about || '' %></textarea>
                                                        </div>
                                                        <div class="form-group">
                                                            <label for="meetingDuration">
                                                                <i class="fas fa-clock text-primary me-2"></i>
                                                                Default Appointment Duration (minutes)
                                                            </label>
                                                            <select class="form-control" id="meetingDuration" name="meetingDuration">
                                                                <option value="15">15 minutes</option>
                                                                <option value="30" selected>30 minutes</option>
                                                                <option value="45">45 minutes</option>
                                                                <option value="60">1 hour</option>
                                                            </select>
                                                        </div>
                                                        <div class="form-actions">
                                                        <button type="submit" class="btn btn-primary">
                                                                <i class="fas fa-save me-2"></i>Save Profile Changes
                                                        </button>
                                                            <button type="button" class="btn btn-outline-secondary ms-2" onclick="settingsPage.resetProfileForm()">
                                                                <i class="fas fa-undo me-2"></i>Reset
                                                            </button>
                                                        </div>
                                                    </form>
                                                </div>
                                                </div>
                                            </div>
                                        </div>

                                    <!-- Contact Settings Tab -->
                                    <div class="settings-tab" id="contact-tab">
                                        <div class="settings-tab-content">
                                            <form id="contactForm" class="settings-form">
                                                <div class="row">
                                                    <div class="col-md-6">
                                                        <div class="form-group">
                                                            <label for="clinicPhone">
                                                                <i class="fas fa-phone text-primary me-2"></i>
                                                                Phone Number
                                                            </label>
                                                            <input type="tel" class="form-control" id="clinicPhone" name="clinicPhone" placeholder="+250 123 456 789">
                                                        </div>
                                                    </div>
                                                    <div class="col-md-6">
                                                        <div class="form-group">
                                                            <label for="clinicAddress">
                                                                <i class="fas fa-map-marker-alt text-primary me-2"></i>
                                                                Street Address
                                                            </label>
                                                            <input type="text" class="form-control" id="clinicAddress" name="clinicAddress" placeholder="Enter street address">
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="row">
                                                    <div class="col-md-6">
                                                        <div class="form-group">
                                                            <label for="clinicSector">
                                                                <i class="fas fa-location-dot text-primary me-2"></i>
                                                                Sector
                                                            </label>
                                                            <select class="form-control" id="clinicSector" name="clinicSector">
                                                                <option value="">Select Sector</option>
                                                                <option value="KK (Kacyiru)">KK (Kacyiru)</option>
                                                                <option value="KG (Gasabo)">KG (Gasabo)</option>
                                                                <option value="KN (Nyarugenge)">KN (Nyarugenge)</option>
                                                                <option value="KM (Muhima)">KM (Muhima)</option>
                                                                <option value="KP (Kicukiro)">KP (Kicukiro)</option>
                                                                <option value="KR (Remera)">KR (Remera)</option>
                                                                <option value="KY (Nyamirambo)">KY (Nyamirambo)</option>
                                                                <option value="KC (Kiyovu)">KC (Kiyovu)</option>
                                                                <option value="KI (Kimihurura)">KI (Kimihurura)</option>
                                                                <option value="KG (Gisozi)">KG (Gisozi)</option>
                                                                <option value="KJ (Gikondo)">KJ (Gikondo)</option>
                                                            </select>
                                                        </div>
                                                    </div>
                                                    <div class="col-md-6">
                                                        <div class="form-group">
                                                            <label for="clinicLocation">
                                                                <i class="fas fa-map text-primary me-2"></i>
                                                                Location Coordinates
                                                            </label>
                                                            <div class="input-group">
                                                                <input type="text" class="form-control" id="clinicLatitude" name="clinicLatitude" placeholder="Latitude" readonly>
                                                                <input type="text" class="form-control" id="clinicLongitude" name="clinicLongitude" placeholder="Longitude" readonly>
                                                                <button type="button" class="btn btn-outline-secondary" onclick="settingsPage.getCurrentLocation()">
                                                                    <i class="fas fa-map-marker-alt"></i>
                                                                </button>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="form-actions">
                                                <button type="submit" class="btn btn-primary">
                                                        <i class="fas fa-save me-2"></i>Save Contact Info
                                                </button>
                                                    <button type="button" class="btn btn-outline-secondary ms-2" onclick="settingsPage.resetContactForm()">
                                                        <i class="fas fa-undo me-2"></i>Reset
                                                    </button>
                                                </div>
                                            </form>
                                        </div>
                                        </div>

                                    <!-- Services Settings Tab -->
                                    <div class="settings-tab" id="services-tab">
                                        <div class="settings-tab-content">
                                            <div class="form-group">
                                                <label>
                                                    <i class="fas fa-stethoscope text-primary me-2"></i>
                                                    Available Services
                                                </label>
                                                <div class="services-grid">
                                                    <div class="form-check">
                                                        <input class="form-check-input" type="checkbox" value="General Medicine" id="service1">
                                                        <label class="form-check-label" for="service1">General Medicine</label>
                                                    </div>
                                                    <div class="form-check">
                                                        <input class="form-check-input" type="checkbox" value="Cardiology" id="service2">
                                                        <label class="form-check-label" for="service2">Cardiology</label>
                                                    </div>
                                                    <div class="form-check">
                                                        <input class="form-check-input" type="checkbox" value="Neurology" id="service3">
                                                        <label class="form-check-label" for="service3">Neurology</label>
                                                    </div>
                                                    <div class="form-check">
                                                        <input class="form-check-input" type="checkbox" value="Pediatrics" id="service4">
                                                        <label class="form-check-label" for="service4">Pediatrics</label>
                                                    </div>
                                                    <div class="form-check">
                                                        <input class="form-check-input" type="checkbox" value="Gynecology" id="service5">
                                                        <label class="form-check-label" for="service5">Gynecology</label>
                                                    </div>
                                                    <div class="form-check">
                                                        <input class="form-check-input" type="checkbox" value="Dermatology" id="service6">
                                                        <label class="form-check-label" for="service6">Dermatology</label>
                                                    </div>
                                                    <div class="form-check">
                                                        <input class="form-check-input" type="checkbox" value="Orthopedics" id="service7">
                                                        <label class="form-check-label" for="service7">Orthopedics</label>
                                                    </div>
                                                    <div class="form-check">
                                                        <input class="form-check-input" type="checkbox" value="ENT" id="service8">
                                                        <label class="form-check-label" for="service8">ENT</label>
                                                    </div>
                                                    <div class="form-check">
                                                        <input class="form-check-input" type="checkbox" value="Dental Care" id="service9">
                                                        <label class="form-check-label" for="service9">Dental Care</label>
                                                    </div>
                                                    <div class="form-check">
                                                        <input class="form-check-input" type="checkbox" value="Laboratory Services" id="service10">
                                                        <label class="form-check-label" for="service10">Laboratory Services</label>
                                                    </div>
                                                    <div class="form-check">
                                                        <input class="form-check-input" type="checkbox" value="Radiology" id="service11">
                                                        <label class="form-check-label" for="service11">Radiology</label>
                                                    </div>
                                                    <div class="form-check">
                                                        <input class="form-check-input" type="checkbox" value="Emergency Care" id="service12">
                                                        <label class="form-check-label" for="service12">Emergency Care</label>
                                                    </div>
                                                    <div class="form-check">
                                                        <input class="form-check-input" type="checkbox" value="Surgery" id="service13">
                                                        <label class="form-check-label" for="service13">Surgery</label>
                                                    </div>
                                                    <div class="form-check">
                                                        <input class="form-check-input" type="checkbox" value="Physical Therapy" id="service14">
                                                        <label class="form-check-label" for="service14">Physical Therapy</label>
                                                    </div>
                                                    <div class="form-check">
                                                        <input class="form-check-input" type="checkbox" value="Mental Health" id="service15">
                                                        <label class="form-check-label" for="service15">Mental Health</label>
                                                    </div>
                                                    <div class="form-check">
                                                        <input class="form-check-input" type="checkbox" value="Nutrition Services" id="service16">
                                                        <label class="form-check-label" for="service16">Nutrition Services</label>
                                                    </div>
                                                    <div class="form-check">
                                                        <input class="form-check-input" type="checkbox" value="Vaccination" id="service17">
                                                        <label class="form-check-label" for="service17">Vaccination</label>
                                                    </div>
                                                    <div class="form-check">
                                                        <input class="form-check-input" type="checkbox" value="Family Planning" id="service18">
                                                        <label class="form-check-label" for="service18">Family Planning</label>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="form-actions">
                                                <button type="button" class="btn btn-primary" onclick="settingsPage.saveServices()">
                                                    <i class="fas fa-save me-2"></i>Save Services
                                                </button>
                                                <button type="button" class="btn btn-outline-secondary ms-2" onclick="settingsPage.resetServices()">
                                                    <i class="fas fa-undo me-2"></i>Reset
                                            </button>
                                            </div>
                                        </div>
                                        </div>

                                    <!-- Schedule Settings Tab -->
                                    <div class="settings-tab" id="schedule-tab">
                                        <div class="settings-tab-content">
                                            <div class="schedule-container">
                                                <div class="row">
                                                    <div class="col-md-6">
                                                        <h5><i class="fas fa-clock text-primary me-2"></i>Working Hours</h5>
                                                        <div class="schedule-item">
                                                            <label>Monday</label>
                                                            <div class="time-inputs">
                                                                <input type="time" class="form-control" id="mondayStart" value="08:00">
                                                                <span>to</span>
                                                                <input type="time" class="form-control" id="mondayEnd" value="17:00">
                                                            </div>
                                                        </div>
                                                        <div class="schedule-item">
